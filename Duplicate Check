def duplicates(aList):
    i =0
    j=1
    while True:
        print i , j
        if aList[i]==aList[j]:
            print "dupe"
            break
        else:
            j = j+1
        if j == len(aList):
            i =i+1
            j=i+1
            
            
        
        
aList = 0,1,2,32,52,4,1,3,6,2,23,7,32,123,854,5,46,21,3,854

duplicates(aList)
